<h1>A Users Table</h1>

<table id="customers">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Created at</th>
    <th>Action</th>
  </tr>
  <tr>
	<% @users.each do |user|%>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.email %></td>
    <td><%= time_ago user.created_at %></td>
    <td><%= link_to "Edit", edit_admin_user_path(user) %>
      <%#= link_to "show", admin_user_path(user) %>
    </td>
  </tr>
	<% end %>
</table>
<br>
